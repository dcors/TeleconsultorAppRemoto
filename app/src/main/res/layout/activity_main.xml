<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.dehcors.teleconsultorapp.MainActivity"
    android:orientation="vertical"
    android:weightSum="1"
    >

    <ImageView
        android:id="@+id/main_logo"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_gravity="right"
        app:srcCompat="@drawable/logo1" />


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="387dp"
        android:layout_margin="8dp">


        <GridLayout
            android:layout_width="296dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|center_horizontal"


            >

            <ImageButton
                android:id="@+id/main_woman"
                android:layout_width="96dp"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_gravity="center"
                android:layout_row="0"
                app:srcCompat="@drawable/girl3b"

                />

            <TextView
                android:id="@+id/txMulher"
                style="@android:style/Widget.DeviceDefault.TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_row="1"
                android:layout_column="0"
                android:layout_marginBottom="8dp"
                android:text="@string/saude_wo"
                android:textColor="@android:color/black" />

            <ImageButton
                android:layout_width="56dp"
                android:layout_height="56dp"
                android:layout_column="1"
                android:layout_gravity="center"
                android:layout_row="0"
                />

            <ImageButton
                android:id="@+id/main_oldMan"
                android:layout_width="96dp"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_gravity="center"
                android:layout_row="0"
                app:srcCompat="@drawable/oldman3" />


            <TextView
                android:id="@+id/txIdoso"
                style="@android:style/Widget.DeviceDefault.TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_row="1"
                android:layout_gravity="center_horizontal"
                android:layout_marginBottom="8dp"
                android:text="@string/saude_vo"
                android:textColor="@android:color/black" />


            <ImageButton
                android:id="@+id/main_bb"
                android:layout_width="96dp"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_gravity="center"
                android:layout_marginTop="18dp"
                android:layout_row="2"
                app:srcCompat="@drawable/baby3" />

            <TextView
                android:id="@+id/txCrianca"
                style="@android:style/Widget.DeviceDefault.TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_row="3"
                android:layout_marginBottom="8dp"
                android:text="@string/saude_bb"
                android:textColor="@android:color/black" />

            <ImageButton
                android:layout_width="56dp"
                android:layout_height="56dp"
                android:layout_column="1"
                android:layout_gravity="center"
                android:layout_row="2"
              />

            <ImageButton
                android:id="@+id/main_primSoc"
                android:layout_width="96dp"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_gravity="center"
                android:layout_marginTop="18dp"
                android:layout_row="2"
                app:srcCompat="@drawable/first_help3" />

            <TextView
                android:id="@+id/txPrimSoc"
                style="@android:style/Widget.DeviceDefault.TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_row="3"
                android:layout_marginBottom="8dp"
                android:text="@string/primSoc"
                android:textColor="@android:color/black" />


            <ImageButton
                android:layout_width="56dp"
                android:layout_height="56dp"
                android:layout_column="1"
                android:layout_gravity="center"
                android:layout_row="4"
                />


            <ImageButton
                android:id="@+id/main_acessarConta"
                style="@android:style/Widget.Material.ImageButton"
                android:layout_width="96dp"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_gravity="center"
                android:layout_marginTop="18dp"
                android:layout_row="4"
                app:srcCompat="@drawable/signin3" />

            <TextView
                android:text="@string/acesse"
                style="@android:style/Widget.DeviceDefault.TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_row="5"
                android:textColor="@color/black"
                android:layout_gravity="center"/>

            <ImageButton
                android:id="@+id/main_criarConta"
                android:layout_width="96dp"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_marginTop="18dp"
                android:layout_row="4"
                android:layout_gravity="center"
                app:srcCompat="@drawable/add3"
              />

            <TextView
                android:text="@string/criar"
                style="@android:style/Widget.DeviceDefault.TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_row="5"
                android:layout_column="2"
                android:textColor="@color/black"
                android:layout_gravity="center"/>


        </GridLayout>

    </FrameLayout>


</LinearLayout>
